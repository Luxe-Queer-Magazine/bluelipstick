{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/manifesto", "dest": "/blue-lipstick-manifesto.html" },
    { "src": "/prelaunch", "dest": "/luxe-queer-prelaunch.html" },
    { "src": "/main", "dest": "/main.html" }
  ]
}
